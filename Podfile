# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target '鲁班零工' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for 鲁班零工
  pod 'JMessage'
  # 网络请求
  pod 'AFNetworking'
  
  # 图片请求
  pod 'SDWebImage', '~> 5.0.0'
  # 支付宝支付sdk
  pod 'AlipaySDK-iOS'
  
  # 下拉刷新
  pod 'MJRefresh'
  
  # 提醒窗,配合MBProgressHUD (MJ)分类更简单
  pod 'MBProgressHUD'
  
  # 相对布局
  pod 'Masonry'
  pod 'SDWebImage'
  pod 'YYModel'
  pod 'YYCache'
  pod 'MJExtension'
  pod 'WechatOpenSDK'
  
  target '鲁班零工Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target '鲁班零工UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
